{
  "version": "4.7.0",
  "sourceRepo": "github.com/strideUX/docflow-template",
  "paths": {
    "content": "docflow"
  },
  "provider": {
    "type": "linear",
    "teamId": null
  },
  "workspace": {
    "activeProjects": [],
    "defaultMilestoneId": null,
    "product": {
      "name": null,
      "labelId": null,
      "icon": null,
      "color": null
    }
  },
  "statusMapping": {
    "BACKLOG": "Backlog",
    "READY": "Todo",
    "IMPLEMENTING": "In Progress",
    "BLOCKED": "Blocked",
    "REVIEW": "In Review",
    "TESTING": "QA",
    "COMPLETE": "Done",
    "ARCHIVED": "Archived",
    "CANCELED": "Canceled",
    "DUPLICATE": "Duplicate"
  },
  "designSystem": null,
  "aiLabor": {
    "enabled": false,
    "defaultProvider": "claude-sonnet-4",
    "thresholds": {
      "warnTokens": 200000,
      "warnCost": 5.00,
      "requireApproval": 10.00
    },
    "tracking": {
      "recordActuals": true,
      "calibrationEnabled": true
    },
    "providerRates": {
      "claude-sonnet-4": {
        "input": 3.00,
        "output": 15.00,
        "inputRatio": 0.70,
        "outputRatio": 0.30
      },
      "claude-opus-4": {
        "input": 15.00,
        "output": 75.00,
        "inputRatio": 0.70,
        "outputRatio": 0.30
      },
      "gpt-4o": {
        "input": 2.50,
        "output": 10.00,
        "inputRatio": 0.70,
        "outputRatio": 0.30
      }
    }
  }
}



